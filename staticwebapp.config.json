{
  "routes": [
    {
      "route": "/react-mfe/*.js",
      "headers": {
        "content-type": "application/javascript"
      }
    },
    {
      "route": "/react-mfe/*.css",
      "headers": {
        "content-type": "text/css"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/react-mfe/*.js", "/react-mfe/*.css", "/react-mfe/*.json"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".css": "text/css"
  }
}